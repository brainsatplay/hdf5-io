{
  "name": "hdf5-io",
  "version": "0.0.15",
  "description": "Simple utility for reading / writing HDF5 files",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "exports": {
    "require": "./dist/index.node.js",
    "import": "./dist/index.esm.js",
    "default": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tinybuild --path tinybuild.demo.js",
    "build": "tinybuild build",
    "typecheck": "tsc --watch --preserveWatchOutput --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand --coverage",
    "build:docs": "cd docs && yarn && yarn run build"
  },
  "keywords": [
    "hdf5",
    "data"
  ],
  "repository": "https://github.com/garrettmflynn/hdf5-io.git",
  "author": "Garrett Flynn <gflynn@usc.edu>",
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "@jest/globals": "^29.4.2",
    "@types/jest": "^29.4.0",
    "h5wasm": "0.4.9",
    "jest": "^29.4.2",
    "node-fetch": "^3.3.0",
    "tinybuild": "0.3.193",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "4.6.4",
    "typescript-lru-cache": "^1.2.3",
    "web-worker": "^1.2.0"
  },
  "dependencies": {
    "visualscript": "^0.0.11"
  }
}
